<table class="table">
  <thead>
    <tr>
      <th>{{=T('Period')}}</th>
      <th>{{=T('Amount')}}</th>
      <th>{{=T('Rate')}}</th>
    </tr>
  </thead>
  <tbody>
    {{for lend in lends[:10]:}}
    <tr>
      <td>{{=lend['period']}}</td>
      <td>{{=lend['amount']}}</td>
      <td>{{=lend['rate']}}</td>
    </tr>
    {{pass}}
  </tbody>
</table>
